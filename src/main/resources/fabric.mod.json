{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}-${mc_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["Discord: yourboykyle", "Wyan (wyannnnn)"],
  "contributors": [
    "zzyyrraa",
    "itplays"
  ],
  "icon": "assets/${mod_id}/logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xyz.yourboykyle.secretroutes.Main"
    ],
    "modmenu": [
      "xyz.yourboykyle.secretroutes.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "mixins.secretroutesmod.fabric.json"
  ],
  "depends": {
    "minecraft": "~${minor_mc_version}",
    "fabricloader": ">=0.15.11",
    "fabric-api": "*",
    "yet_another_config_lib_v3": ">=3.8.1+1.21.10-fabric",
    "hypixel-mod-api": ">=1.0.1"
  },
  "custom": {
    "modmenu": {
      "update_checker": true,
      "links": {
        "GitHub": "https://github.com/yourboykyle/SecretRoutes"
      }
    }
  }
}